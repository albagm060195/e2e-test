{
  "name": "e2e-test",
  "version": "1.0.0",
  "description": "OCARIoT platform e2e test.",
  "main": "index.js",
  "scripts": {
    "cypress:open": "cypress open",
    "test:chrome": "(npm run pretest && cypress run --browser chrome) || npm run posttest",
    "clean:reports": "rm -R -f cypress/reports && mkdir cypress/reports && mkdir cypress/reports/mochareports",
    "pretest": "npm run clean:reports",
    "scripts": "cypress run",
    "combine-reports": "mochawesome-merge --reportDir cypress/reports/mocha > cypress/reports/mochareports/report.json",
    "generate-report": "marge cypress/reports/mochareports/report.json -f report -o cypress/reports/mochareports",
    "posttest": "npm run combine-reports && npm run generate-report",
    "test": "npm run scripts || npm run posttest"
  },
  "repository": {
    "type": "git",
    "url": "http://douglas@200.129.82.7:8000/nutes-ocariot/server/e2e-test.git"
  },
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@types/mongoose": "^5.5.6",
    "@types/node": "^11.15.10",
    "cypress": "^4.5.0",
    "cypress-multi-reporters": "^1.2.3",
    "dotenv": "^8.2.0",
    "mocha": "^7.1.2",
    "mochawesome": "^4.1.0",
    "mochawesome-merge": "^2.0.1",
    "mochawesome-report-generator": "^4.1.0",
    "mongoose": "5.8.2",
    "mysql2": "^2.1.0",
    "sequelize": "^5.21.5"
  }
}
